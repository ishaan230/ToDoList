<%- include("header") %>

    <div class="box" id="heading">
    <h1><%= listTitle %>!</h1>
    </div>
    <div class="box">
        <% for (var i = 0;i<newlistitem.length;i++){ %>
            <div class="item">
                <input type="checkbox">
                <p><%= newlistitem[i]%></p>
            </div>
        <% } %>
        <form action="/" method="post"class="item">
            <input type="text" name="newitem" placeholder="Add Item">
            <button type="submit" name="button">+</button>
        </form>
        </div>

<%- include("footer") %>
